public OnPlayerDeath(playerid, killerid, reason)
{
	CheckPlayerFlood(playerid, true, MAX_FLOOD_RATE, 500, FLOOD_RATE_KICK);
	
	if(GetPlayerData(playerid, P_JAIL) == 0)
	{
	    new rand_spawn = random(2) + 1;

