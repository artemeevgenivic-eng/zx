CMD:fvig(playerid, params[])
{
	if(!(1 <= GetPlayerTeamEx(playerid) < MAX_ORG)) return SendClientMessage(playerid, 0x999999FF, "Вам недоступна данная функция");
	if(!(9 <= GetPlayerJob(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Функция доступна лидерам организации и их заместителям");

	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /fvig [id игрока] [причина]");

	extract params -> new to_player, string:reason[32];
	
	new uid = GetPlayerAccountID(to_player);
	new warns = GetPlayerData(to_player, P_OWARN);

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0xCECECEFF, ""red_palka"Такого игрока нет");

	if(!(1 <= strlen(reason) <= 32))
		return SendClientMessage(playerid, 0xCECECEFF, ""red_palka"Введите причину");

	if(GetPlayerTeamEx(to_player) != GetPlayerTeamEx(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, ""red_palka"Этот игрок не состоит в Вашей организации");

	if(GetPlayerJob(to_player) >= GetPlayerJob(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Нельзя выдать выговор заместителю/лидеру организации");
	
	AddPlayerData(to_player, P_OWARN, +, 1);
	
	new fmt_msg[115];
    format(fmt_msg, sizeof fmt_msg, "%s %s выдал выговор игроку %s [%d/3]. Причина: %s", GetPlayerJobAndRankName(playerid), GetPlayerNameEx(playerid), GetPlayerNameEx(to_player), GetPlayerData(to_player, P_OWARN), reason);
	SendMessageToTeam(GetPlayerTeamEx(playerid), fmt_msg, 0x3399FFFF);

	if(warns == 2)
	{
		SendClientMessage(to_player, 0xFF5533FF, ""red_palka"Вы были исключены из организации. Причина: 3/3 выговора.");
		
    	format(fmt_msg, sizeof fmt_msg, "%s %s был уволен из организации из-за большого количества выговоров", GetPlayerJobAndRankName(to_player), GetPlayerNameEx(to_player));
		SendMessageToTeam(GetPlayerTeamEx(playerid), fmt_msg, 0x3399FFFF);

		format(fmt_msg, sizeof fmt_msg, "UPDATE accounts SET owarn=0,team=0,job=0 WHERE id=%d", uid);
		mysql_query(mysql, fmt_msg, false);
		
		SetPlayerData(to_player, P_OWARN, 0);
		SetPlayerData(to_player, P_TEAM, 0);
		SetPlayerData(to_player, P_JOB, 0);
		SetPlayerSkin(to_player, GetPlayerData(to_player, P_SKIN));
        SetPlayerColorEx(to_player, 0xFFFFFFFF);
		SetPlayerData(to_player, P_OWARN, 0);
		SetPlayerData(to_player, P_DIVISION, 0);
		UpdatePlayerDatabaseInt(to_player, "division", GetPlayerDivisionEx(to_player));
		
		UnInvite(playerid, to_player);
	}

	format(fmt_msg, sizeof fmt_msg, "UPDATE accounts SET owarn=%d WHERE id=%d", warns, uid);
	mysql_query(mysql, fmt_msg, false);


	return 1;
}

