stock HudDateTime(playerid, action)
{
    new BitStream:bitstream = BS_New();
    BS_WriteValue(bitstream, PR_UINT8, PACKET_CUSTOMRPC);
    BS_WriteValue(bitstream, PR_UINT32, 0x72);

	new year, month, day;
	getdate(year, month, day);
	
	new hour, minute;
	gettime(hour, minute);

    new datetime[100];
    
    if(0 <= minute < 10)
    {
        format_chat
		format(chat, sizeof chat, "%04d-%02d-%02d %d:0%d", year, month, day, hour, minute);
		strcat(datetime, chat);
    }
    else
	{
	    format_chat
		format(chat, sizeof chat, "%04d-%02d-%02d %d:%d", year, month, day, hour, minute);
		strcat(datetime, chat);
	}

    BS_WriteValue(bitstream, PR_UINT16, strlen(datetime));
    BS_WriteValue(bitstream, PR_STRING, datetime);

    BS_WriteValue(bitstream, PR_INT32, action);

    PR_SendPacket(bitstream, playerid);

    BS_Delete(bitstream);

    return true;
}

