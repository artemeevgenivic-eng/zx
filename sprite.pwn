public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
    CheckPlayerFlood(playerid, true, MAX_FLOOD_RATE, 500, FLOOD_RATE_KICK);
    
    if(dialogid == DIALOG_JOB_BUS_INVITE)
	{
		if(response)
		{
			if(GetPlayerLevel(playerid) < 2) return SCM(playerid, -1, ""ye_palka"Чтобы устроиться в компанию, необходим 2 уровень");

			new companyid = GetPVarInt(playerid, "bus_job_companyid");

			SCM(playerid, -1, "Вы успешно устроились в автобусную компанию");

			SetPlayerData(playerid, P_COMPJOB_BUS, companyid);
			UpdatePlayerDatabaseInt(playerid, "companyjob_bus", companyid);

			format_chat
			format(chat, sizeof chat, "[Bus-Company] Гражданин %s устроился в вашу компанию", GetPlayerNameEx(playerid));
			SCMC(GetPlayerCompJobBusEx(playerid), chat, 1);
		}
		else DeletePVar(playerid, "bus_job_companyid");
	}

    if(dialogid == DIALOG_JOB_BUS)
 	{
        if(response)
        {
		new companyid = GetPlayerData(playerid, P_COMPJOB_BUS);

		if(listitem == 0)
		{
			new dialogstr[450];

			if(GetCompanyData(companyid, C_VECTOR) == 0)
			{
				strcat(dialogstr, "В компании отсутствует транспорт\n");

				Dialog
				(
					playerid, DIALOG_JOB_BUS_1, DIALOG_STYLE_LIST,
					"Аренда транспорта",
