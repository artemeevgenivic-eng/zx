	new query[256];

	mysql_format(mysql, query, sizeof query, "UPDATE accounts SET `%s`='%f' WHERE `id`=%d LIMIT 1", field, value, GetPlayerAccountID(playerid));
	mysql_query(mysql, query, false);
	return 1;
}

