stock ShowVideo(playerid, const url[], time, skip)
{
	// 0 - видео по URL адресу без кнопки пропустить
	// 1 - видео по URL адресу с кнопкой пропустить
	// 2 - видео из кеша AzureGames/media/bus-owner.mp4 без кнопки пропустить
	// 3 - видео из кеша AzureGames/media/bus-owner.mp4 с кнопкой пропустить

	new BitStream:bitstream = BS_New();
	BS_WriteValue(bitstream, PR_UINT8, PACKET_CUSTOMRPC);
	BS_WriteValue(bitstream, PR_UINT32, 0x76);

	BS_WriteValue(bitstream, PR_UINT16, strlen(url));
   	BS_WriteValue(bitstream, PR_STRING, url);

	BS_WriteValue(bitstream, PR_INT32, time);

	BS_WriteValue(bitstream, PR_INT32, skip);

	PR_SendPacket(bitstream, playerid);

	BS_Delete(bitstream);

	if(time != 0)
	{
		ShowLoading(playerid, time);
	}

	if(test_server)
	{
		SCM(playerid, -1, ""rpc"ShowVideo");
	}

	return true;
}

