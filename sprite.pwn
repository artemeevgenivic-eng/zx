CMD:follow(playerid, params[])
{
	new team = GetPlayerData(playerid, P_TEAM);
	if(team != TEAM_MVD && team != TEAM_FBI && team != TEAM_ARMY)
		return SendClientMessage(playerid, 0x999999FF, "Вы не сотрудник");
	
	if(GetPlayerData(playerid, P_DAYORG) == 1) 
		return SendClientMessage(playerid, 0x999999FF, ""red_palka"Начните рабочий день");
	
	if(isnull(params))
	{
		if(g_FollowedBy[playerid] != INVALID_PLAYER_ID)
		{
			new targetid = g_FollowedBy[playerid];
			
			g_Following[targetid] = INVALID_PLAYER_ID;
			g_FollowedBy[playerid] = INVALID_PLAYER_ID;
			
			SendClientMessage(playerid, 0xFFFF00FF, "Вы перестали вести игрока");
			SendClientMessage(targetid, 0xFFFF00FF, "Вас больше не ведут");
		}
		else
		{
			SendClientMessage(playerid, 0xCECECEFF, "Используйте: /follow [id игрока]");
		}
		return 1;
	}
	
	new targetid;
	if(sscanf(params, "u", targetid)) 
		return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /follow [id игрока]");
	
	if(!IsPlayerConnected(targetid) || !IsPlayerLogged(targetid)) 
		return SendClientMessage(playerid, 0x999999FF, "Такого игрока нет");
	
	if(!IsPlayerInRangeOfPlayer(playerid, targetid, 5.0)) 
		return SendClientMessage(playerid, 0x999999FF, "Игрок находится слишком далеко");
	
	if(!GetPlayerData(targetid, P_CUFFED)) 
		return SendClientMessage(playerid, 0x999999FF, "Игрок не закован в наручники");
	
	if(g_FollowedBy[playerid] != INVALID_PLAYER_ID)
	{
		if(g_FollowedBy[playerid] == targetid)
		{
			g_Following[targetid] = INVALID_PLAYER_ID;
			g_FollowedBy[playerid] = INVALID_PLAYER_ID;
			
			SendClientMessage(playerid, 0xFFFF00FF, "Вы перестали вести игрока");
			SendClientMessage(targetid, 0xFFFF00FF, "Вас больше не ведут");
			return 1;
		}
		else
		{
			new old_target = g_FollowedBy[playerid];
			g_Following[old_target] = INVALID_PLAYER_ID;
			g_FollowedBy[playerid] = INVALID_PLAYER_ID;
			
			SendClientMessage(playerid, 0xFFFF00FF, "Вы перестали вести предыдущего игрока");
			SendClientMessage(old_target, 0xFFFF00FF, "Вас больше не ведут");
		}
	}
	
	if(g_Following[targetid] != INVALID_PLAYER_ID)
	{
		new old_leader = g_Following[targetid];
		g_FollowedBy[old_leader] = INVALID_PLAYER_ID;
		g_Following[targetid] = INVALID_PLAYER_ID;
		
		SendClientMessage(old_leader, 0xFFFF00FF, "Вы больше не ведете игрока");
		SendClientMessage(targetid, 0xFFFF00FF, "Вас теперь ведет другой сотрудник");
	}
	
	g_Following[targetid] = playerid;
	g_FollowedBy[playerid] = targetid;
	GetPlayerPos(playerid, g_LastFollowPos[playerid][0], g_LastFollowPos[playerid][1], g_LastFollowPos[playerid][2]);
	SendClientMessage(playerid, 0x00FF00FF, "Вы начали вести игрока. Используйте /follow без параметров чтобы остановить");
	SendClientMessage(targetid, 0x00FF00FF, "Вас ведут. Вы будете следовать за сотрудником");
	
	return 1;
}

CMD:unfollow(playerid, params[])
{
	if(g_FollowedBy[playerid] != INVALID_PLAYER_ID)
	{
		new targetid = g_FollowedBy[playerid];
		
		g_Following[targetid] = INVALID_PLAYER_ID;
		g_FollowedBy[playerid] = INVALID_PLAYER_ID;
		
		SendClientMessage(playerid, 0xFFFF00FF, "Вы перестали вести игрока");
		SendClientMessage(targetid, 0xFFFF00FF, "Вас больше не ведут");
	}
	else
	{
		SendClientMessage(playerid, 0x999999FF, "Вы никого не ведете");
	}
	return 1;
}

forward FollowTimer();
public FollowTimer()
{
	foreach(new i : Player)
	{
		if(g_Following[i] != INVALID_PLAYER_ID)
		{
			new leader = g_Following[i];
			
			if(!IsPlayerConnected(leader) || !IsPlayerLogged(leader))
			{
				g_Following[i] = INVALID_PLAYER_ID;
				if(g_FollowedBy[leader] == i) g_FollowedBy[leader] = INVALID_PLAYER_ID;
				continue;
			}
			
			if(!IsPlayerInPoliceTeam(leader) && GetPlayerData(leader, P_TEAM) != TEAM_FBI && GetPlayerData(leader, P_TEAM) != TEAM_ARMY)
			{
				g_Following[i] = INVALID_PLAYER_ID;
				g_FollowedBy[leader] = INVALID_PLAYER_ID;
				SendClientMessage(i, 0xFFFF00FF, "Вас больше не ведут сотрудник больше не на службе");
				continue;
			}
			
			if(!GetPlayerData(i, P_CUFFED))
			{
				g_Following[i] = INVALID_PLAYER_ID;
				g_FollowedBy[leader] = INVALID_PLAYER_ID;
				SendClientMessage(leader, 0xFFFF00FF, "Игрок снял наручники, ведение прекращено");
				continue;
			}
			
			new Float:leader_x, Float:leader_y, Float:leader_z;
			GetPlayerPos(leader, leader_x, leader_y, leader_z);
			
			new Float:distance = GetPlayerDistanceFromPoint(i, leader_x, leader_y, leader_z);
			
			if(distance > 10.0)
			{
				new Float:angle;
				GetPlayerFacingAngle(leader, angle);
				
				new Float:offset_x = leader_x - 1.5 * floatsin(angle, degrees);
				new Float:offset_y = leader_y + 1.5 * floatcos(angle, degrees);
				
				SetPlayerPos(i, offset_x, offset_y, leader_z);
				SetPlayerFacingAngle(i, angle);
				
				g_LastFollowPos[leader][0] = leader_x;
				g_LastFollowPos[leader][1] = leader_y;
				g_LastFollowPos[leader][2] = leader_z;
			}
			else if(distance > 3.0)
			{
				new Float:angle = atan2(leader_y - g_LastFollowPos[leader][1], leader_x - g_LastFollowPos[leader][0]);
				
				new Float:speed = 3.0; 
				new Float:new_x = leader_x - 1.5 * floatsin(angle, degrees);
				new Float:new_y = leader_y + 1.5 * floatcos(angle, degrees);
				
				SetPlayerVelocity(i, (new_x - leader_x) * speed, (new_y - leader_y) * speed, 0.0);
				
				SetPlayerFacingAngle(i, angle);
				
				g_LastFollowPos[leader][0] = leader_x;
				g_LastFollowPos[leader][1] = leader_y;
				g_LastFollowPos[leader][2] = leader_z;
			}
			
			if(GetPlayerVirtualWorld(i) != GetPlayerVirtualWorld(leader))
				SetPlayerVirtualWorld(i, GetPlayerVirtualWorld(leader));
			
			if(GetPlayerInterior(i) != GetPlayerInterior(leader))
				SetPlayerInterior(i, GetPlayerInterior(leader));
		}
	}
}