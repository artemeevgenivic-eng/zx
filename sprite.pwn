	new string[128];
	format(string, sizeof(string), "Вам предложили купить медкарту!, с ее помощью вы можете\nустраиватся в организации/работы\n\nЖелаете приобрести ее по цене: %d?", price);
	ShowPlayerDialog(to_player, 2312, DIALOG_STYLE_MSGBOX, "Покупка Мед-Карты", string, "Купить", "Отказатся");
	pers2[playerid] = playerid;
