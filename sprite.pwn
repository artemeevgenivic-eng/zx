CMD:server(playerid)
{
	if(GetPlayerAdminEx(playerid) <= 5) return ShowNotification(playerid, 5, "Вам не доступна данная команда", 3, "", ">>");
	
	new query[190], Cache: result;

	mysql_format(mysql, query, sizeof query, "SELECT * FROM server LIMIT 1");
	result = mysql_query(mysql, query, true);

	new openserverSQL = cache_get_field_content_int(0, "server_open");

    cache_delete(result);
	
	if(openserverSQL == 1)
	{
	    SendClientMessage(playerid, -1, ""blue_palka"Вы успешно закрыли доступ к подключению на сервер");
		new	fmt_st[500];
		format(fmt_st, sizeof fmt_st, ""adm_log"[SERVER-CLOSE] Администратор %s закрыл доступ к подключению на сервер", GetPlayerNameEx(playerid));
		SendMessageToAdmins(fmt_st, 0xFF5533FF);
		
		mysql_format(mysql, query, sizeof query, "UPDATE server SET server_open = 0");
		mysql_query(mysql, query, false);
		
		for(new playersid = 0; playersid < MAX_PLAYERS; playersid++)
		{
		    if(GetPlayerDostupEx(playersid) == 0)
		    {
				Kick(playersid);
		    }
		}
	}
	else
	{
	    SendClientMessage(playerid, -1, ""blue_palka"Вы успешно открыли доступ к подключению на сервер");
        new	fmt_str[500];
		format(fmt_str, sizeof fmt_str, ""adm_log"[SERVER-CLOSE] Администратор %s открыл доступ к подключению на сервер", GetPlayerNameEx(playerid));
		SendMessageToAdmins(fmt_str, 0xFF5533FF);
		
		mysql_format(mysql, query, sizeof query, "UPDATE server SET server_open = 1");
		mysql_query(mysql, query, false);
	}
	
	new hour,minute,second;
	gettime(hour,minute,second);
	
	new year, month,day;
	getdate(year, month, day);
	
	new qwery[500];
	format(qwery, 256, "INSERT INTO `admins_log` (`id`,`name`,`admlvl`,`cmd`,`komment`,`time`,`data`) VALUES ('%d','%s','%d','/server','Переменная openserver: %d','%d:%d:%d','%04d-%02d-%02d')", GetPlayerAccountID(playerid), GetPlayerNameEx(playerid), GetPlayerAdminEx(playerid), openserverSQL, hour, minute, second, year, month, day);
	mysql_query(mysql, qwery, false);
	
	return 1;
}

CMD:givedostup(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) <= 5) return ShowNotification(playerid, 5, "Вам не доступна данная команда", 3, "", ">>");

  	extract params -> new string: to_player[21], dostup; else return SendClientMessage(playerid, COR_SERVER, ""ye_palka"Используйте /givedostup [ник игрока] [0 - забрать, 1 - выдать]");

  	if(dostup >= 2) return SendClientMessage(playerid, COR_SERVER, ""ye_palka"Используйте /givedostup [ник игрока] [0 - забрать, 1 - выдать]");

	if(dostup == 0)
	{
	    format_chat
		format(chat, sizeof chat, ""ye_palka"Вы успешно вынесли игрока %s из вайт листа", to_player);
		SCM(playerid, 0xFF5533FF, chat);
		
		format(chat, sizeof chat, ""adm_log"[WHITE-LIST] Администратор %s успешно вынес игрока %s из вайт листа", GetPlayerNameEx(playerid), to_player);
		SendMessageToAdmins(chat, 0xFF5533FF);
		
		format(chat, sizeof chat, "Забрал вайт лист игроку %s", to_player);
		AdmLog(playerid, "/givedostup", chat);
	}
	else
	{
	    format_chat
		format(chat, sizeof chat, ""ye_palka"Вы успешно внесли игрока %s в вайт лист", to_player);
		SCM(playerid, 0xFF5533FF, chat);
		
		format(chat, sizeof chat, ""adm_log"[WHITE-LIST] Администратор %s успешно внес игрока %s в вайт лист", GetPlayerNameEx(playerid), to_player);
		SendMessageToAdmins(chat, 0xFF5533FF);
		
		format(chat, sizeof chat, "Выдал вайт лист игроку %s", to_player);
		AdmLog(playerid, "/givedostup", chat);
	}

  	new fmt_str[85];
	format(fmt_str, sizeof fmt_str, "UPDATE accounts SET dostup=%d WHERE name='%s' LIMIT 1", dostup, to_player);
	mysql_query(mysql, fmt_str, false);

	return 1;
}

