stock CreateVehicles() // машины мода
{
    for(new idx; idx < sizeof create_veh_salon; idx ++)
	{
		vehsalon = CreateVehicle(create_veh_salon[idx][A_CARID], create_veh_salon[idx][A_POS_X], create_veh_salon[idx][A_POS_Y], create_veh_salon[idx][A_POS_Z], create_veh_salon[idx][A_POS_A], 25, 25, -1, 1, create_veh_salon[idx][A_TYPECAR]);
		CreateDynamic3DTextLabel(create_veh_salon[idx][A_NAME], 0xffe373AA, create_veh_salon[idx][A_POS_X], create_veh_salon[idx][A_POS_Y], create_veh_salon[idx][A_POS_Z] + 1.3, 10.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, create_veh_salon[idx][A_WORLD], create_veh_salon[idx][A_INTERIOR]);
		CreateDynamic3DTextLabel(create_veh_salon[idx][A_PRICENAME], 0xffe373AA, create_veh_salon[idx][A_POS_X], create_veh_salon[idx][A_POS_Y], create_veh_salon[idx][A_POS_Z] + 1.0, 10.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, create_veh_salon[idx][A_WORLD], create_veh_salon[idx][A_INTERIOR]);
		LinkVehicleToInterior(vehsalon, create_veh_salon[idx][A_INTERIOR]);
		SetVehicleVirtualWorld(vehsalon, create_veh_salon[idx][A_WORLD]);
	}
}

