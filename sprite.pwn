	format(fmt_text, sizeof fmt_text, ""ye_palka"%s объявил Вас в розыск. Причина: %s. Уровень розыска %d", GetPlayerNameEx(playerid), message, GetPlayerSuspect(criminalid));
	SendClientMessage(criminalid, 0x3399FFFF, fmt_text);
	callcmd::hudwanted(criminalid);
	
	format(fmt_text, sizeof fmt_text, ""green_palka"Вы объявили %s в розыск. Причина: %s. Уровень розыска %d", GetPlayerNameEx(criminalid), message, GetPlayerSuspect(criminalid));
