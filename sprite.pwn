CMD:givecoins(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) <= 4) return ShowNotification(playerid, 5, "Вам не доступна данная команда", 3, "", ">>");

	extract params -> new to_player, coins; else return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /givecoins [id игрока] [cумма]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return ShowNotification(playerid, 5, "Такого игрока нет", 3, "", ">>");

	if(!(-25_000 <= coins <= 1_000)) return SendClientMessage(playerid, 0xCECECEFF, "Можно выдавать от 1 до 1000 Coins за один раз");

	AddPlayerData(to_player, P_COINS, +, coins);
    UpdatePlayerDatabaseInt(to_player, "coins", GetPlayerCoinsEx(to_player));

    new string[300];
    
	format(string, sizeof(string), ""ye_palka"Администратор %s выдал Вам %d Coins", GetPlayerNameEx(playerid), coins);
	SendClientMessage(to_player, 0xFFFFFFFF, string);

	format(string, sizeof(string), ""adm_log"[A] %s[%d] выдал Coins %s[%d] кол-во %d Coins, в итоге у игрока %d Coins",
	GetPlayerNameEx(playerid), playerid, GetPlayerNameEx(to_player), to_player, coins, GetPlayerCoinsEx(to_player));
	SendMessageToAdmins(string, 0xFFFF00FF);

	new hour,minute,second;
	gettime(hour,minute,second);
	new year, month,day;
	getdate(year, month, day);

	new qwery[500];
	format(qwery, 256, "INSERT INTO `admins_log` (`id`,`name`,`admlvl`,`cmd`,`komment`,`time`,`data`) VALUES ('%d','%s','%d','/givecoins','Выдал %d Coins игроку %s[%d], %d Coins','%d:%d:%d','%04d-%02d-%02d')", GetPlayerAccountID(playerid), GetPlayerNameEx(playerid), GetPlayerAdminEx(playerid), coins, GetPlayerNameEx(to_player), GetPlayerAccountID(to_player), GetPlayerCoinsEx(to_player), hour, minute, second, year, month, day);
	mysql_query(mysql, qwery, false);

	return 1;
}

