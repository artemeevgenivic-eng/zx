IPacket:0x75(playerid, BitStream:bs)
{
	new header, rpcid, action;
	BS_ReadValue(bs, PR_UINT8, header, PR_UINT32, rpcid);
	BS_ReadValue(bs, PR_INT32, action);
	
	if(action == 1)
	{
	    SetPlayerData(playerid, P_PRINTS, 1);
	    SetTimer("prints", 1000, false);
	    SetPlayerChatBubble(playerid, "печатает...", 0xffea4fAA, 10.0, 2000);
	}
	if(action == 0)
	{
	    SetPlayerData(playerid, P_PRINTS, 0);
	}

	return 1;
}

