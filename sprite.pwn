stock SCMC(player_company, message[], to_company)
{
	new count;
	
	if(to_company == 1) // сообщение Bus компании
	{
	    	foreach(new playerid : Player)
		{
			if(GetPlayerCompJobBusEx(playerid) == player_company)
			{
			    if(!IsPlayerLogged(playerid)) continue;

				SendClientMessage(playerid, 0x006b0bAA, message);
				count ++;
