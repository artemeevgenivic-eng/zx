	}
	else if(!strcmp(text, ":)", true))
	{
		Action(playerid, "улыбнулся", _, false);
	}
	else if(!strcmp(text, ":(", true))
	{
		Action(playerid, "расстроился", _, false);
	}
	else if(!strcmp(text, ":D", true))
	{
		Action(playerid, "смеётся", _, false);
	}
	else if(!strcmp(text, "+", true))
	{
		Action(playerid, "согласен", _, false);
	}
	else if(!strcmp(text, "-", true))
	{
		Action(playerid, "не согласен", _, false);
	}
	else if(!strcmp(text, "?", true))
	{
		Action(playerid, "спрашивает", _, false);
	}
	else if(!strcmp(text, "!", true))
	{
		Action(playerid, "сердится", _, false);
	}
	else if(!strcmp(text, ".", true))
	{
		Action(playerid, "молчит", _, false);
	}
	else SendMessageInChat(playerid, text);

	return 0;
}

