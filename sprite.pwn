                        SendLog(offer_id, LOG_TYPE_FRACTION, fmt_msg);

                    }
					case OFFER_TYPE_HANDSHAKE:
					{
						if(!(IsPlayerInAnyVehicle(playerid) && IsPlayerInAnyVehicle(offer_id)))
